@model IEnumerable<string>
@{ Layout = null;}

<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="width=devce-width" />
    <title>Language Features</title>
</head>

<body>

    <ul>
        @foreach (string s in Model)
        {
            <li>@s</li>
        }
    </ul>

</body>
</html>